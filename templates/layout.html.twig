<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

        <link rel="apple-touch-icon" sizes="180x180" href="/assets/apple-touch-icon.png">
        <link rel="icon" type="image/png" sizes="32x32" href="/assets/favicon-32x32.png">
        <link rel="icon" type="image/png" sizes="16x16" href="/assets/favicon-16x16.png">
        <link rel="manifest" href="/assets/manifest.json">
        <link rel="mask-icon" href="/assets/safari-pinned-tab.svg" color="#5c3878">
        <link rel="shortcut icon" href="/assets/favicon.ico">
        <meta name="msapplication-config" content="/assets/browserconfig.xml">
        <meta name="theme-color" content="#5c3878">
        {% block meta %}{% endblock %}

    {% if app.environment == "prod" %}
        <link rel="stylesheet" href="{{ asset_hash( '/assets/css/main.d3729d6c.css') }}"></link>
    {% endif %}

        {% block title %}<title>Frontastic Online Shop</title>{% endblock %}
    </head>
    <body>
    {% block main %}
        <noscript>
            You need to enable JavaScript to run this application.
        </noscript>
        <div id="root"></div>
    {% endblock %}
    {% if app.environment == "dev" %}
        <script type="text/javascript" src="/webpack/js/bundle.js"></script>
    {% else %}
        <script type="text/javascript" src="{{ asset_hash( '/assets/js/main.d3729d6c.js') }}"></script>
    {% endif %}
    </body>
</html>
