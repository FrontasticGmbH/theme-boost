    "module": "src/js/index.js",
    "scripts": {
        "start": "node bin/start.js",
        "build": "node bin/build.js",
        "test": "node bin/test.js --env=jsdom",
        "check": "node node_modules/.bin/eslint --ext .js --ext .jsx -c ../../../../paas/integration/eslint.config.json src/js/ test/js/",
        "fix": "node node_modules/.bin/eslint --fix --ext .js --ext .jsx -c ../../../../paas/integration/eslint.config.json src/js/ test/js/",
        "server:start": "NODE_ENV=development nodemon --legacy-watch --watch src/js/ bin/devServer.js --exec babel-node --presets env,react-app",
        "storybook": "start-storybook -s ./public -p 9001",
        "build-storybook": "build-storybook"
    },
    "babel": {
        "plugins": [
            "@babel/plugin-proposal-class-properties",
            "@babel/plugin-syntax-dynamic-import"
        ],
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "<rootDir>/src/js/**/*.{js,jsx}"
        ],
        "setupFiles": [
            "<rootDir>/../../../catwalk/config/polyfills.js"
        ],
        "testMatch": [
            "<rootDir>/test/js/**/*.js?(x)"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx)$": "babel-jest",
            "^.+\\.css$": "<rootDir>/../../../catwalk/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/../../../catwalk/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "json",
            "web.jsx",
            "jsx"
        ]
    },
    "browserslist": [
        "last 1 version",
        "> 1%",
        "not ie <= 8"
    ],
    "eslintConfig": {
        "extends": "react-app"
    }
