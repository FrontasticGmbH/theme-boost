                    "default": ""
                },
                {
                    "label": "List",
                    "field": "listItems",
                    "type": "group",
                    "itemLabelField": "label",
                    "min": 1,
                    "fields": [
                        {
                            "label": "Label",
                            "field": "label",
                            "required": true,
                            "type": "string"
                        },
                        {
                            "label": "Icon",
                            "field": "icon",
                            "required": false,
                            "type": "media"
                        }
                    ]
                }
            ]
        }
    ]
}
