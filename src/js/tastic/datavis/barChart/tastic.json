{
    "tasticType": "datavis-barChart",
    "name": "Bar Chart",
    "icon": "timeline",
    "schema": [
        {
            "name": "Chart General",
            "fields": [
                {
                    "label": "Show X Axis",
                    "field": "showXAxis",
                    "type": "boolean",
                    "default": true
                },
                {
                    "label": "Show Y Axis",
                    "field": "showYAxis",
                    "type": "boolean",
                    "default": true
                },
                {
                    "label": "Show Horizontal Grid Lines",
                    "field": "showHorizontalGridLines",
                    "type": "boolean",
                    "default": true
                },
                {
                    "label": "Show Vertical Grid Lines",
                    "field": "showVerticalGridLines",
                    "type": "boolean",
                    "default": true
                }
            ]
        },
        {
            "name": "Chart Specific",
            "fields": [
                {
                    "label": "Chart Height",
                    "field": "chartHeight",
                    "type": "integer",
                    "default": 300
                },
                {
                    "label": "Bar Fill Color (For total series)",
                    "field": "barFill",
                    "type": "string",
                    "default": "#707070",
                    "translatable": false
                }
            ]
        }
    ]
}